{
    "name": "fabpot/silex-skeleton",
    "description": "A pre-configured skeleton for the Silex microframework",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2",
        "silex/silex": "*",
        "silex/web-profiler": "*",
        "symfony/asset": "*",
        "symfony/browser-kit": "*",
        "symfony/class-loader": "*",
        "symfony/config": "*",
        "symfony/filesystem": "*",
        "symfony/console": "*",
        "symfony/css-selector": "*",
        "symfony/debug": "*",
        "symfony/finder": "*",
        "symfony/monolog-bridge": "*",
        "symfony/process": "*",
        "symfony/security": "*",
	"symfony/form": "<=3.5",
        "symfony/translation": "<4.3.10",
        "symfony/twig-bridge": "*",
        "symfony/validator": "*",
	"symfony/doctrine-bridge": "*",
        "doctrine/dbal": "*",
	"doctrine/doctrine-bundle": "<=2.10",
        "twig/twig": "*",
        "dflydev/doctrine-orm-service-provider": "*",
        "doctrine/persistence": "*",
        "doctrine/orm": "*",
        "symfony/string": "*",
        "geocoder-php/locationiq-provider": "^1.0",
        "php-http/guzzle6-adapter": "^2.0",
        "saxulum/saxulum-doctrine-orm-manager-registry-provider": "^3.1",
        "silex/providers": "^2.3",
        "pimple/pimple": "^3.3"
    },
    "autoload": {
        "psr-0": {
		"": "src/",
		"Przystanki\\Przystanek": "src/models/",
		"Geocoding\\Geocoder":  "src/models/"

         }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "scripts": {
        "run": [
            "echo 'Started web server on http://localhost:8888'",
            "php -S localhost:8888 -t web"
        ]
    }
}
