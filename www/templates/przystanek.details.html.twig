<div class="bus-stop-details">
    {% block content %}
        <h4>{{ przystanek.nazwa }}</h4>
        <p>📅 {{ przystanek.data }} ( 💻 {{ przystanek.ip }}) - 🌐 {{ przystanek.browser }}  🚏  {{ przystanek.adres }}
        </p>
        <p>{{ przystanek.opis }}</p>
        <button data-id="{{ przystanek.id }}" type="button" class="btn btn-dark btn-hide">Ukryj</button>
    {% endblock %}
</div>